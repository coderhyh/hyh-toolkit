{
  "name": "hyh-toolkit",
  "type": "module",
  "version": "0.4.1",
  "description": "这是一个多功能工具库，集成了 Vite Plugin、Vue Hook、Utils，为开发提供便利。",
  "author": "coderhyh <coderhyh@gmail.com>",
  "license": "MIT",
  "homepage": "https://coderhyh.github.io/hyh-toolkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderhyh/hyh-toolkit"
  },
  "bugs": "https://github.com/coderhyh/hyh-toolkit/issues",
  "keywords": [
    "tool",
    "toolkit",
    "hook",
    "vite plugin",
    "utils",
    "vue",
    "vite"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsup-node",
    "clean": "rimraf dist lib es types",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commit": "git add . && cz",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "docs:dev": "vitepress dev packages",
    "docs:build": "vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "init": "pnpm i && git init && pnpm run prepare && git add . && git commit -m 'init'"
  },
  "config": {
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "dependencies": {
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.11.9",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.1",
    "bumpp": "^9.1.1",
    "commitizen": "4.3.0",
    "cz-customizable": "7.0.0",
    "element-plus": "^2.3.8",
    "eslint": "^8.57.0",
    "husky": "^8.0.0",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.1",
    "tsup": "^8.0.2",
    "typescript": "^4.8.4",
    "unocss": "^0.55.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.4",
    "vitepress": "1.0.0-beta.5",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  }
}
