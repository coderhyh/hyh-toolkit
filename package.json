{
  "name": "hyh-toolkit",
  "version": "0.1.3",
  "description": "tools",
  "author": "coderhyh Fu <coderhyh@gmail.com>",
  "license": "MIT",
  "homepage": "https://coderhyh.github.io/hyh-toolkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderhyh/hyh-toolkit"
  },
  "bugs": "https://github.com/coderhyh/hyh-toolkit/issues",
  "keywords": [
    "tool",
    "toolkit",
    "hook",
    "vitePlugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite-plugin": {
      "types": "./dist/vite-plugin.d.ts",
      "require": "./dist/vite-plugin.js",
      "import": "./dist/vite-plugin.mjs"
    },
    "./resolve": {
      "types": "./dist/resolve.d.ts",
      "require": "./dist/resolve.js",
      "import": "./dist/resolve.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup --watch src",
    "release": "bump --tag --commit \"release: v%s\" --push",
    "start": "ts-node src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts src/ --fix",
    "commit": "git add . && cz",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "init": "pnpm i && git init && pnpm run prepare && git add . && git commit -m 'init'"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "commitizen": "4.2.4",
    "cz-customizable": "6.3.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "ts-loader": "^9.4.1",
    "tsup": "^7.1.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.4.4",
    "vitepress": "1.0.0-beta.5",
    "vitepress-plugin-pagefind": "^0.2.4",
    "vue": "^3.3.4",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chokidar": "^3.5.3"
  }
}
